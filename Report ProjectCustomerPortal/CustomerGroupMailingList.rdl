  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@GroupNameParam">
            <Value>=Parameters!GroupNameParam.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT GP.GroupName, CC.CustNum, CUST.CustomerName, CC.Name, CC.Title, CC.Email
	FROM CrmContact CC
	INNER JOIN Customer CUST ON CUST.AceId = CC.CustNum
	INNER JOIN CRMCustomerGroup GP ON GP.CustNum = CC.CustNum
	WHERE GP.GroupName = @GroupNameParam 
	  AND CC.Active = 1 
	  AND CUST.Active = 1
	  AND CC.Email IS NOT NULL 
	  AND (CC.UnsubMarketingEmails IS NULL OR CC.UnsubMarketingEmails = 0)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="GroupName">
          <DataField>GroupName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CustNum">
          <DataField>CustNum</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CustomerName">
          <DataField>CustomerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Title">
          <DataField>Title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Email">
          <DataField>Email</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSetGroups">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT DISTINCT GroupName FROM CRMCustomerGroup ORDER BY GroupName</CommandText>
      </Query>

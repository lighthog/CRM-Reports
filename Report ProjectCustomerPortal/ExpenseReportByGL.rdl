    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSetExpense">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EmployeeID">
            <Value>=Parameters!EmployeeID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ProfitabilityPeriodID">
            <Value>=Parameters!ProfitabilityPeriodID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
				
SELECT        Employee.EmployeeFullName, CRMCustomerProspect.Name AS Customer, ExpenseItems.CustNum, ExpenseItems.ExpenseDate, 
                         ExpenseItems.ExpenseCategory, ExpenseItems.Mileage, ExpenseItems.Amount, ExpenseItems.Description, ExpenseItems.ExpenseID, 
                         ExpenseItems.Approved, ExpenseItems.ApprovedBy, ExpenseItems.ReceiptFileName, ISNULL(P.Description,'Unapproved') AS GLPeriod
FROM            ExpenseItems INNER JOIN
                         Employee ON ExpenseItems.EmployeeID = Employee.EmployeeID LEFT OUTER JOIN
                         CRMCustomerProspect ON CRMCustomerProspect.CustNum = ExpenseItems.CustNum LEFT OUTER JOIN
                         ProfitabilityPeriod AS P ON ExpenseItems.DateApproved &gt;= P.BeginDate AND ExpenseItems.DateApproved &lt;= P.EndDate
WHERE        ((P.ProfitabilityPeriodID =@ProfitabilityPeriodID) OR (P.ProfitabilityPeriodID IS NULL)) 
				AND (ExpenseItems.EmployeeID = @EmployeeID)</CommandText>
      </Query>
      <Fields>
        <Field Name="EmployeeFullName">
          <DataField>EmployeeFullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Customer">
          <DataField>Customer</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CustNum">
          <DataField>CustNum</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ExpenseDate">
          <DataField>ExpenseDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="ExpenseCategory">
          <DataField>ExpenseCategory</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Mileage">
          <DataField>Mileage</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Amount">
          <DataField>Amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ExpenseID">
          <DataField>ExpenseID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Approved">
          <DataField>Approved</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="ApprovedBy">
          <DataField>ApprovedBy</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ReceiptFileName">
          <DataField>ReceiptFileName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="GLPeriod">
          <DataField>GLPeriod</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSetGLPeriod">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT        Description AS MonthYear, ProfitabilityPeriodID
FROM            ProfitabilityPeriod
ORDER BY BeginDate</CommandText>
      </Query>
      <Fields>
        <Field Name="MonthYear">
          <DataField>MonthYear</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProfitabilityPeriodID">
          <DataField>ProfitabilityPeriodID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSetCurrentGLPeriod">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT        ProfitabilityPeriodID
FROM            ProfitabilityPeriod
WHERE        (GETDATE() &gt;= BeginDate) AND (GETDATE() &lt;= EndDate)</CommandText>
      </Query>
      <Fields>
        <Field Name="ProfitabilityPeriodID">
          <DataField>ProfitabilityPeriodID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>

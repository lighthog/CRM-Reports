  <DataSets>
    <DataSet Name="dsTotalComponents">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@pInspector">
            <Value>=Parameters!pInspector.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT COUNT(DISTINCT(IC.InspectionComponentID)) AS TotalComponents
   FROM Inspection I
   INNER JOIN InspectionEquipment IE ON I.InspectionID = IE.InspectionID
   INNER JOIN InspectionComponent IC ON IC.InspectionEquipmentID = IE.InspectionEquipmentID 
   INNER JOIN ComponentPointData CPD ON CPD.InspectionComponentID = IC.InspectionComponentID
   WHERE CPD.Condition IN ('F','P','U') 
        AND IE.Inspector = @pInspector  
        AND I.InspectionDate &gt; '1/1/2012'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="TotalComponents">
          <DataField>TotalComponents</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsTotalComments">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@pInspector">
            <Value>=Parameters!pInspector.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT COUNT(DISTINCT(IC.InspectionComponentID)) AS TotalComments
   FROM Inspection I
   INNER JOIN InspectionEquipment IE ON I.InspectionID = IE.InspectionID
   INNER JOIN InspectionComponent IC ON IC.InspectionEquipmentID = IE.InspectionEquipmentID 
   INNER JOIN ComponentPointData CPD ON CPD.InspectionComponentID = IC.InspectionComponentID
   WHERE CPD.Condition IN ('F','P','U') 
        AND IE.Inspector = @pInspector 
        AND I.InspectionDate &gt; '1/1/2012'
        AND LEN(Comment) &gt; 8 </CommandText>
      </Query>
      <Fields>
        <Field Name="TotalComments">
          <DataField>TotalComments</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsTotalRepairDescs">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@pInspector">
            <Value>=Parameters!pInspector.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        COUNT(DISTINCT IC.InspectionComponentID) AS TotalRepairDescs
FROM            Inspection AS I INNER JOIN
                         InspectionEquipment AS IE ON I.InspectionID = IE.InspectionID INNER JOIN
                         InspectionComponent AS IC ON IC.InspectionEquipmentID = IE.InspectionEquipmentID INNER JOIN
                         ComponentPointData AS CPD ON CPD.InspectionComponentID = IC.InspectionComponentID
WHERE        (CPD.Condition IN ('F', 'P', 'U')) AND (IE.Inspector = @pInspector) AND (I.InspectionDate &gt; '1/1/2012') AND (LEN(CPD.RepairDesc) &gt; 8)</CommandText>
      </Query>
      <Fields>
        <Field Name="TotalRepairDescs">
          <DataField>TotalRepairDescs</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsTotalPhotos">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@pInspector">
            <Value>=Parameters!pInspector.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT COUNT(DISTINCT(IC.InspectionComponentID)) AS TotalPhotos
   FROM Inspection I
   INNER JOIN InspectionEquipment IE ON I.InspectionID = IE.InspectionID
   INNER JOIN InspectionComponent IC ON IC.InspectionEquipmentID = IE.InspectionEquipmentID 
   INNER JOIN ComponentPointData CPD ON CPD.InspectionComponentID = IC.InspectionComponentID
   INNER JOIN ComponentPointDataPicture CPDP ON CPDP.ComponentPointDataID = CPD.ComponentPointDataID
   WHERE CPD.Condition IN ('F','P','U') 
        AND IE.Inspector = @pInspector 
        AND I.InspectionDate &gt; '1/1/2012'
        AND LEN(Comment) &gt; 8 </CommandText>
      </Query>
      <Fields>
        <Field Name="TotalPhotos">
          <DataField>TotalPhotos</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsInspectors">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT DISTINCT Inspector FROM InspectionEquipment 
	WHERE DateInspected &gt; '1/1/2012' AND LEN(Inspector) &gt; 1 AND Inspector IS NOT NULL 
	ORDER BY Inspector</CommandText>
      </Query>
      <Fields>
        <Field Name="Inspector">
          <DataField>Inspector</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>

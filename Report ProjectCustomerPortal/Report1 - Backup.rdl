      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT DISTINCT 
                         TS.Name AS CompanyName, TS.Addr, TS.City, TS.St, TS.Zip, TS.CustCat, TS.CustNum, TS.BrNum AS Branch, TS.Active, TS.PhNum AS CompanyPH, 
                         CRM.Name AS ContactName, CRM.Title, CRM.Phone1 AS ContactPh1, CRM.Phone2 AS ContactPh2, CRM.Fax AS ContactFax, CRM.Email, 
                         CRM.Address AS ContactAddress, CRM.City AS ContactCity, CRM.State AS ContactSt, CRM.Zip AS ContactZip, TS.Slsp, CG.GroupName, TSarShipto.Name AS ST_Name, 
                         TSarShipto.City AS ST_City, TSarShipto.St AS ST_State, TSarShipto.Zip AS ST_Zip
FROM            TSarCustomer AS TS INNER JOIN
                         CRMContact AS CRM ON TS.CustNum = CRM.CustNum INNER JOIN
                         CRMCustomerGroup AS CG ON CRM.CustNum = CG.CustNum INNER JOIN
                         TSarShipto ON TS.CustNum = TSarShipto.CustNum
WHERE        (TS.CustCat IN (@CustomerCategory)) AND (TS.Active = 1) AND (CRM.Active = 1) AND (CRM.Email IS NOT NULL) AND (CRM.Email &lt;&gt; ' ') AND 
                         (TS.CustNum LIKE @IntOnly) AND (CG.GroupName IN (@GroupName) OR
                         CG.GroupName IS NULL)
ORDER BY CG.GroupName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>

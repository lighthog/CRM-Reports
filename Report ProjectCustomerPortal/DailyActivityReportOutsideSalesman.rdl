        </QueryParameters>
        <CommandText>SELECT        B.Name AS Customer, CRMContact.Name AS Contact, A.Note, A.Date, A.CustNum
FROM            CRMNote AS A INNER JOIN
                         ProfitabilityPeriod AS P ON A.Date &gt;= P.BeginDate AND A.Date &lt;= P.EndDate INNER JOIN
                         CRMCustomerProspect AS B ON B.CustNum = A.CustNum INNER JOIN
                         CRMContact ON A.CRMContactID = CRMContact.CRMContactID
WHERE        (P.ProfitabilityPeriodID = @ProfitabilityPeriodID) AND (A.LoggedBy = @Slsp)</CommandText>
      </Query>

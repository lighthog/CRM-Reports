  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@pFromDate">
            <Value>=Parameters!FromDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@pToDate">
            <Value>=Parameters!ToDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@EmpID">
            <Value>=Parameters!EmpID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Approved">
            <Value>=Parameters!Approved.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  Employee.EmployeeFullName, CRMCustomerProspect.Name AS Customer, ExpenseItems.CustNum, ExpenseItems.ExpenseDate, ExpenseItems.DateApproved, 
		ExpenseItems.ExpenseCategory, ExpenseItems.Mileage, ExpenseItems.Amount, ExpenseItems.Description, ExpenseItems.ExpenseID, 
        ExpenseItems.Approved, ExpenseItems.ApprovedBy, ExpenseItems.ReceiptFileName
	FROM ExpenseItems 
	INNER JOIN Employee ON ExpenseItems.EmployeeID = Employee.EmployeeID 
	LEFT OUTER JOIN CRMCustomerProspect ON CRMCustomerProspect.CustNum = ExpenseItems.CustNum 
	WHERE DateApproved BETWEEN @pFromDate AND @pToDate
                     AND Employee.EmployeeID IN (@EmpID) 
                     AND ExpenseItems.Approved IN (@Approved)
               ORDER BY Employee.EmployeeFullName, ExpenseDate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>

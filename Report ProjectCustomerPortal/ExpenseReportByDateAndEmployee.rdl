    <DataSet Name="dsEmployees">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT DISTINCT EMP.EmployeeFullName, EMP.EmployeeID 
	FROM Employee EMP
	INNER JOIN ExpenseItems EXI ON EXI.EmployeeID = EMP.EmployeeID
	WHERE EXI.DateApproved &gt; DATEADD(mm, -13, GetDate())
	ORDER BY EMP.EmployeeFullName</CommandText>
      </Query>

	FROM CrmContact CC
	INNER JOIN Customer CUST ON CUST.AceId = CC.CustNum
	INNER JOIN CRMCustomerGroup GP ON GP.CustNum = CC.CustNum
	WHERE GP.GroupName = @GroupNameParam 
	  AND CC.Active = 1 
	  AND CUST.Active = 1
	  AND CC.Email IS NOT NULL 
	  AND (CC.UnsubMarketingEmails IS NULL OR CC.UnsubMarketingEmails = 0)</CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query><Fields><Field Name="GroupName"><DataField>GroupName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="CustNum"><DataField>CustNum</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="CustomerName"><DataField>CustomerName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Name"><DataField>Name</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Title"><DataField>Title</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Email"><DataField>Email</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="DataSetGroups"><Query><DataSourceName>AceWOMDataSource</DataSourceName><CommandText>SELECT DISTINCT GroupName FROM CRMCustomerGroup ORDER BY GroupName</CommandText></Query><Fields><Field Name="GroupName"><DataField>GroupName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields></DataSet></DataSets><ReportParameters><ReportParameter Name="GroupNameParam"><DataType>String</DataType><Prompt>Group:</Prompt><ValidValues><DataSetReference><DataSetName>DataSetGroups</DataSetName><ValueField>GroupName</ValueField><LabelField>GroupName</LabelField></DataSetReference></ValidValues></ReportParameter></R
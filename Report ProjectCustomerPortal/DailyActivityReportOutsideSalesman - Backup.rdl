    <DataSet Name="NoteActivitySummary">
      <Fields>
        <Field Name="Count">
          <DataField>Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="NoteCategory">
          <DataField>NoteCategory</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT        COUNT(*) AS Count, CRMNote.NoteCategory
FROM            CRMNote INNER JOIN
                         ProfitabilityPeriod AS P ON CRMNote.Date &gt;= P.BeginDate AND CRMNote.Date &lt;= P.EndDate
WHERE        (P.ProfitabilityPeriodID = @ProfitabilityPeriodID) AND (CRMNote.LoggedBy = @Slsp)
GROUP BY CRMNote.NoteCategory
ORDER BY Count DESC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProfitabilityPeriodID">
            <Value>=Parameters!ProfitabilityPeriodID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Slsp">
            <Value>=Parameters!Slsp.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Statistics">
      <Fields>
        <Field Name="Label">
          <DataField>Label</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Count">
          <DataField>Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Ordinal">
          <DataField>Ordinal</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT        'Unique Customer Touches' AS Label, COUNT(DISTINCT CRMNote.CustNum) AS Count, 1 AS Ordinal
FROM            CRMNote INNER JOIN
                         ProfitabilityPeriod AS P ON CRMNote.Date &gt;= P.BeginDate AND CRMNote.Date &lt;= P.EndDate
WHERE        (P.ProfitabilityPeriodID = @ProfitabilityPeriodID) AND (CRMNote.LoggedBy = @Slsp)
UNION
SELECT        'Unique Contact Touches' AS Label, COUNT(DISTINCT CRMNote_1.CRMContactID) AS Count, 2 AS Ordinal
FROM            CRMNote AS CRMNote_1 INNER JOIN
                         ProfitabilityPeriod AS P ON CRMNote_1.Date &gt;= P.BeginDate AND CRMNote_1.Date &lt;= P.EndDate
WHERE        (P.ProfitabilityPeriodID = @ProfitabilityPeriodID) AND (CRMNote_1.LoggedBy = @Slsp)
UNION
SELECT        'New Contacts' AS Label, COUNT(*) AS Count, 3 AS Ordinal
FROM            CRMContact INNER JOIN
                         ProfitabilityPeriod AS P ON CRMContact.DateLogged &gt;= P.BeginDate AND CRMContact.DateLogged &lt;= P.EndDate
WHERE        (P.ProfitabilityPeriodID = @ProfitabilityPeriodID) AND (CRMContact.LoggedBy = @Slsp)
UNION
SELECT        'New Qualified Accounts' AS Label, COUNT(*) AS Count, 4 AS Ordinal
FROM            TSarCustomerExtension AS A INNER JOIN
                         ProfitabilityPeriod AS P ON A.DateQualified &gt;= P.BeginDate AND A.DateQualified &lt;= P.EndDate
WHERE        (P.ProfitabilityPeriodID = @ProfitabilityPeriodID) AND (A.Slsp = @Slsp)
UNION
SELECT        'New Prospect Accounts' AS Label, COUNT(*) AS Count, 5 AS Ordinal
FROM            CRMProspect INNER JOIN
                         ProfitabilityPeriod AS P ON CRMProspect.DateLogged &gt;= P.BeginDate AND CRMProspect.DateLogged &lt;= P.EndDate
WHERE        (P.ProfitabilityPeriodID = @ProfitabilityPeriodID) AND (CRMProspect.LoggedBy = @Slsp)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProfitabilityPeriodID">
            <Value>=Parameters!ProfitabilityPeriodID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Slsp">
            <Value>=Parameters!Slsp.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DailyActivitySummary">
      <Fields>
        <Field Name="Customer">
          <DataField>Customer</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Contact">
          <DataField>Contact</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Note">
          <DataField>Note</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Date">
          <DataField>Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="CustNum">
          <DataField>CustNum</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT        B.Name AS Customer, CRMContact.Name AS Contact, A.Note, A.Date, A.CustNum
FROM            CRMNote AS A INNER JOIN
                         ProfitabilityPeriod AS P ON A.Date &gt;= P.BeginDate AND A.Date &lt;= P.EndDate INNER JOIN
                         CRMCustomerProspect AS B ON B.CustNum = A.CustNum INNER JOIN
                         CRMContact ON A.CRMContactID = CRMContact.CRMContactID
WHERE        (P.ProfitabilityPeriodID = @ProfitabilityPeriodID) AND (A.LoggedBy = @Slsp)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProfitabilityPeriodID">
            <Value>=Parameters!ProfitabilityPeriodID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Slsp">
            <Value>=Parameters!Slsp.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="NewOrders">
      <Fields>
        <Field Name="Amt">
          <DataField>Amt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT        SUM(Sales) AS Amt
FROM            QualifiedSalesByPayPeriodSlsp
WHERE        (ProfitabilityPeriodID = @ProfitabilityPeriodID) AND (Slsp = @Slsp)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProfitabilityPeriodID">
            <Value>=Parameters!ProfitabilityPeriodID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Slsp">
            <Value>=Parameters!Slsp.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>

  <DataSets>
    <DataSet Name="QualifiedQuotes">
      <Fields>
        <Field Name="QuoteSales">
          <DataField>QuoteSales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="NumberOfQuotes">
          <DataField>NumberOfQuotes</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT        SUM(SOMerchAmt) AS QuoteSales, COUNT(*) AS NumberOfQuotes
FROM            QualifiedQuotes
WHERE        (Slsp = @Slsp) AND (ProfitabilityPeriodID = @ProfitabilityPeriodID)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Slsp">
            <Value>=Parameters!Slsp.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ProfitabilityPeriodID">
            <Value>=Parameters!ProfitabilityPeriodID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="GLPeriods">
      <Fields>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProfitabilityPeriodID">
          <DataField>ProfitabilityPeriodID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT        Description, ProfitabilityPeriodID
FROM            ProfitabilityPeriod
ORDER BY BeginDate</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="DataSetGetPeriod">
      <Fields>
        <Field Name="CurrentPeriod">
          <DataField>CurrentPeriod</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT        MIN(ProfitabilityPeriodID) - 1 AS CurrentPeriod
FROM            ProfitabilityPeriod
WHERE        (GETDATE() &lt; BeginDate)</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="OutsideSalesman">
      <Fields>
        <Field Name="EmployeeFullName">
          <DataField>EmployeeFullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SQL_Login">
          <DataField>SQL_Login</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT        EmployeeFullName, SQL_Login
FROM            Employee
WHERE        (Position = 'Salesperson (Outside)') AND (Terminated = 0)
ORDER BY EmployeeFullName</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="PeriodInformation">
      <Fields>
        <Field Name="BeginDate">
          <DataField>BeginDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="EndDate">
          <DataField>EndDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT        BeginDate, EndDate, Description
FROM            ProfitabilityPeriod
WHERE        (ProfitabilityPeriodID = @ProfitabilityPeriodID)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProfitabilityPeriodID">
            <Value>=Parameters!ProfitabilityPeriodID.Value</Value>
          </QueryParameter>

  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="EmployeeFullName">
          <DataField>EmployeeFullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PayType">
          <DataField>PayType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Hours">
          <DataField>Hours</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT E.EmployeeFullName, PayType, SUM(ETS.Hours) AS Hours
	FROM EmployeeTimeSalary ETS
	INNER JOIN Employee E ON E.EmployeeID = ETS.EmployeeID
	INNER JOIN PayPeriod PP ON PP.StartDate &lt;= ETS.Date AND PP.EndDate &gt;= ETS.Date 
	WHERE PP.PayPeriodID = 206
	GROUP BY E.EmployeeFullName, PayType
	HAVING SUM(ETS.Hours) BETWEEN 4 AND 79.9
UNION
SELECT E.EmployeeFullName,  PayType, SUM(TLI.HourDiff) AS Hours
	FROM TimeLineItem TLI
	INNER JOIN Employee E ON E.EmployeeID = TLI.EmployeeID
	INNER JOIN PayPeriod PP ON PP.StartDate &lt;= TLI.BeginTime AND PP.EndDate &gt;= TLI.BeginTime
	WHERE PP.PayPeriodID = 206
	GROUP BY E.EmployeeFullName, PayType
	HAVING SUM(TLI.HourDiff) BETWEEN 4 AND 79.9
ORDER BY EmployeeFullName, PayType</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>

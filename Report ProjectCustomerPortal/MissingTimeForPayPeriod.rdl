  <DataSources>
    <DataSource Name="AceWOMDataSource">
      <DataSourceReference>AceWOMDataSource</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>b2b8fb84-c3f4-40b1-b84d-a5b40a97c94b</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PayPeriod">
            <Value>=Parameters!rpPayPeriod.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT E.EmployeeFullName, PayType, 'Salaried' AS HoursType, SUM(ETS.Hours) AS Hours
	FROM EmployeeTimeSalary ETS
	INNER JOIN Employee E ON E.EmployeeID = ETS.EmployeeID
	INNER JOIN PayPeriod PP ON PP.StartDate &lt;= ETS.Date AND PP.EndDate &gt;= ETS.Date 
	WHERE PP.PayPeriodID = @PayPeriod
	GROUP BY E.EmployeeFullName, PayType
	HAVING SUM(ETS.Hours) BETWEEN 4 AND 79.9
UNION
SELECT E.EmployeeFullName,  PayType, 'Hourly' AS HoursType, SUM(TLI.HourDiff) AS Hours
	FROM TimeLineItem TLI
	INNER JOIN Employee E ON E.EmployeeID = TLI.EmployeeID
	INNER JOIN PayPeriod PP ON PP.StartDate &lt;= TLI.BeginTime AND PP.EndDate &gt;= TLI.BeginTime
	WHERE PP.PayPeriodID = @PayPeriod
	GROUP BY E.EmployeeFullName, PayType
	HAVING SUM(TLI.HourDiff) BETWEEN 4 AND 79.9
ORDER BY EmployeeFullName, PayType</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="EmployeeFullName">
          <DataField>EmployeeFullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PayType">
          <DataField>PayType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Hours">
          <DataField>Hours</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="HoursType">
          <DataField>HoursType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsPayPeriods">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT PayPeriodID, CAST(StartDate AS VARCHAR(12)) + '- ' + CAST(EndDate AS VARCHAR(12)) AS DateText 
     FROM PayPeriod 
     WHERE StartDate BETWEEN DATEADD(d, -60, GETDATE()) AND GETDATE()</CommandText>
      </Query>
      <Fields>
        <Field Name="PayPeriodID">
          <DataField>PayPeriodID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DateText">
          <DataField>DateText</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsCurrentPayPeriod">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT TOP 1 PayPeriodID 
     FROM PayPeriod 
     WHERE EndDate &gt; DATEADD(d, -7, GETDATE()) ORDER BY StartDate ASC</CommandText>
      </Query>
      <Fields>
        <Field Name="PayPeriodID">
          <DataField>PayPeriodID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>

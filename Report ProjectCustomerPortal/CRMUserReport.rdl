        </QueryParameters>
        <CommandText>SELECT        CRMNote.LoggedBy, CRMNote.NoteCategory, CRMNote.Note, CRMCustomerProspect.Name AS CustName, CRMContact.Name AS ContactName, 
                         CRMNote.CRMContactID, CRMNote.CustNum, CRMNote.DateLogged, Employee.EmployeeFullName, CRMNote.Date
FROM            CRMNote INNER JOIN
                         CRMCustomerProspect ON CRMNote.CustNum = CRMCustomerProspect.CustNum INNER JOIN
                         Employee ON Employee.SQL_Login = CRMNote.LoggedBy LEFT OUTER JOIN
                         CRMContact ON CRMNote.CRMContactID = CRMContact.CRMContactID
WHERE        (CRMNote.DateLogged &gt; DATEADD(d, CAST(@Difference AS int), GETDATE()))</CommandText>

  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DaysBack">
            <Value>=Parameters!DaysBack.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Employee">
            <Value>=Parameters!Employee.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Type">
            <Value>=Parameters!Type.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@MinAmount">
            <Value>=Parameters!MinAmount.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@MaxAmount">
            <Value>=Parameters!MaxAmount.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT E.EmployeeFullName, COUNT(Q.ID) AS TotalQuotes, SUM(Q.Won) AS TotalWon
    FROM Employee E 
    LEFT OUTER JOIN EmployeeCustomer EC ON E.EmployeeID = EC.EmployeeID
    LEFT OUTER JOIN CRMQuotesTSConsolidated Q ON Q.CustNum = EC.CustNum 
    LEFT OUTER JOIN TSarCustomer CUST ON CUST.CustNum = EC.CustNum
    WHERE EC.EmployeeID IN  (@Employee)
         AND Q.Dept IS NOT NULL 
         AND Q.QuoteDate &gt;DATEADD(d,(0-@DaysBack),GetDate())
         AND Q.Dept IN (@Type)
         AND (Q.QuoteAmount &gt; @MinAmount AND Q.QuoteAmount &lt; @MaxAmount)
     GROUP BY E.EmployeeFullName
     ORDER BY  E.EmployeeFullName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="EmployeeFullName">
          <DataField>EmployeeFullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalQuotes">
          <DataField>TotalQuotes</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TotalWon">
          <DataField>TotalWon</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>
SELECT Dept, COUNT(*) FROM CRMQuotesTSConsolidated WHERE Dept IS NOT NULL GROUP BY Dept HAVING COUNT(*) &gt; 30</CommandText>
      </Query>
      <Fields>
        <Field Name="Dept">
          <DataField>Dept</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ID">
          <DataField>Dept</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Employees">
      <Query>
        <DataSourceName>AceWOMDataSource</DataSourceName>
        <CommandText>SELECT E.EmployeeID, E.EmployeeFullName FROM aspnet_Users U 
	INNER JOIN aspnet_UsersInRoles UR ON U.UserId = UR.UserId 
	INNER JOIN aspnet_Roles R ON R.RoleId = UR.RoleId 
	INNER JOIN Employee E ON U.UserName = 'ACEWORLD\'+E.SQL_Login
	WHERE R.RoleName = 'CRMUser' AND E.Active = 1
	ORDER BY E.EmployeeFullName;</CommandText>
      </Query>
      <Fields>
        <Field Name="EmployeeID">
          <DataField>EmployeeID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="EmployeeFullName">
          <DataField>EmployeeFullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
